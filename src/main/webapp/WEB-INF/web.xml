<web-app>
  <display-name>User Registration Application</display-name>
  <servlet>
    <servlet-name>Register</servlet-name>
    <servlet-class>servlet.RegistrationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Register</servlet-name>
    <url-pattern>/check</url-pattern>
